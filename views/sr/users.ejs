<%- include ('b'); %>
	Korisnici:<br/>
	<% let code=``; -%>
	<% for (let i=0;i<rows.length;i++){ -%>
		Ime: <%= rows[i].name %> <%= rows[i].surname -%><br/>
		<% if (rows[i].email != 'undefined'){ -%>
			Email: <%= rows[i].email -%><br/>
		<% } -%>
		Modmin: <a id="<%= i %>"><%= rows[i].type %></a><% if (user.id=='3343576799017822' && rows[i].type != user.id){ %> <button type="button" class="btn btn-primary" id="<%= i %>inc">Povecaj</button> <button type="button" class="btn btn-primary" id="<%= i %>dec">Smanji</button><br/>
			<% code+=`document.getElementById("${i}inc").addEventListener("click",()=>{` -%>
				<% code+=`let el=document.getElementById("${i}");` -%>
				<% code+=`let type=Number(el.innerHTML);` -%>
				<% code+=`if (type < 2){` -%>
					<% code+=`el.innerHTML=++type;` -%>
					<% code+=`req({data:'type='+type+'&id=${rows[i].id}'});` -%>
					<% code+=`}` -%>
			<% code+=`});` -%>
			<% code+=`document.getElementById("${i}dec").addEventListener("click",()=>{` -%>
				<% code+=`let el=document.getElementById("${i}");` -%>
				<% code+=`let type=Number(el.innerHTML);` -%>
				<% code+=`if (type > 0){` -%>
					<% code+=`el.innerHTML=--type;` -%>
					<% code+=`req({data:'type='+type+'&id=${rows[i].id}'});` -%>
				<% code+=`}` -%>
			<% code+=`});` -%>
	<% }} -%>
